{
  "name": "ascii-cell",
  "version": "0.1.0",
  "description": "A frontend library for making cellular automatons, rendered as ASCII art!",
  "repository": "https://github.com/adri326/ascii-cell/",
  "author": "Shad Amethyst",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./simulation": {
      "types": "./dist/simulation.d.ts",
      "default": "./dist/simulation.js"
    },
    "./font": {
      "types": "./dist/font.d.ts",
      "default": "./dist/font.js"
    },
    "./effect": {
      "types": "./dist/effect.d.ts",
      "default": "./dist/effect.js"
    },
    "./color": {
      "types": "./dist/color.d.ts",
      "default": "./dist/color.js"
    },
    "./wire": {
      "types": "./dist/wire.d.ts",
      "default": "./dist/wire.js"
    }
  },
  "dependencies": {
    "base64-js": "^1.5.1"
  },
  "devDependencies": {
    "solid-js": "^1.9.3",
    "@shadryx/pptk": "^0.1.10",
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-plugin-solid": "^2.11.0"
  }
}
